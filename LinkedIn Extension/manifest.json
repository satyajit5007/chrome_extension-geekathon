{
  "name": "Linkedin Voice Command",
  "description": "A chrome extension for efficient internet browsing",
  "version": "1.0",
  "manifest_version": 3,

  "icons": {
    "16": "/img/16.png",
    "32": "/img/32.png",
    "48": "/img/48.png"
  },

  "content_scripts": [
    {
      "js": ["/content.js","popup.js"],
      "css": ["/content.css"],
      "matches": ["https://www.linkedin.com/*"]
    }
  ],
  
  "web_accessible_resources": [
    {
      "resources": ["img/save.png"],
      "matches": ["https://www.linkedin.com/*"]
    }
  ]

  
}
