{
  "name": "Simiple Voice Command",
  "description": "A chrome extension for efficient internet browsing",
  "version": "1.0",
  "manifest_version": 3,

  "icons": {
    "16": "/img/16.png",
    "32": "/img/32.png",
    "48": "/img/48.png"
  },

  "content_scripts": [
    {
      "js": ["/popup.js","/content.js"],
      "css": ["/content.css"],
      "matches": [
        "*://*/*"
      ]
    }
  ]
}
